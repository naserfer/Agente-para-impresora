{
  "name": "print-agent",
  "version": "1.0.0",
  "description": "Agente de impresión local para sistema de lomiterías - Soporte para impresoras térmicas",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:with-gui": "concurrently \"npm run dev\" \"cd ../desktop && npm run dev\" --names \"AGENT,GUI\" --prefix-colors \"blue,green\"",
    "pm2:start": "pm2 start server.js --name print-agent",
    "pm2:stop": "pm2 stop print-agent",
    "pm2:restart": "pm2 restart print-agent",
    "test:ticket": "node test-ticket.js",
    "test:print": "node test-ticket.js --print"
  },
  "keywords": [
    "printer",
    "escpos",
    "thermal",
    "ticket",
    "lomiteria"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.86.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "escpos": "^3.0.0-alpha.6",
    "escpos-network": "^3.0.0-alpha.4",
    "escpos-usb": "^3.0.0-alpha.4",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "iconv-lite": "^0.7.0",
    "pdf-to-printer": "^5.6.1",
    "printer": "^0.4.0",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.1"
  }
}
